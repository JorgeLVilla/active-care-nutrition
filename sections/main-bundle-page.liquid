<style> 

  .bundle_text {
    position: absolute;
    max-width: 40%;
    display: flex;
    flex-direction: column;
    align-items: start !important;
    left: 20px !important;
    padding: 20px !important;
    bottom: 13% !important;
    border-radius: 24px;
    min-width: 624px;
  }

  .bundle-hero {
    width: 100%;
  }
  
    .desktop-hero-image {
      display: flex;
    }

    .mobile-hero-image {
      display: none;
    }

    .floating-hero-image {
      position: absolute;
      right: 0%;
      width: 65%;
      bottom: -15%;
    }

  .bundle_nd_sv {
    margin-bottom: 60px;
  }
  
  @media screen and (max-width: 1200px) { 
  .bundle_text { 
    background: rbga(255,255,255,0.5) !important
  }

    .desktop-hero-image {
      display: none;
    }

    
    .mobile-hero-image {
      display: flex;
    }
  }

  .bundle_img img {
    width: 100%;
    min-height: 600px;
    max-height: 600px;
/*     object-fit: cover;
    object-position: right; */
}

  .bundle_text h2.bundle-main-heading {
    font-size: 40px;
    color: #262F69;
    font-weight: 900;
    margin-bottom: 0px !important;
    padding-bottom: 0px !important

  }

  .bundle-top-heading {
    font-size: 24px;
    color: #262F69;
    font-weight: 500;
  }
  
  .bundle-bottom-heading {
    font-size: 32px;
    color: #262F69;
    font-weight: 500;
  }

  .bundle-description {
    font-size: 18px;
    color: #262F69;
    font-weight: 500;
    margin-top: 10px;
  }

  .bundle_text .secnd_btnshbnl {
    margin: 30px 0px 10px 0px !important;
    font-size: 18px;
    text-align: center;
    font-weight: 700;
    padding: 5px 60px;
  }

  .hero-logo-grid {
    display: grid;
    grid-template-columns: 30% 30% 30%;
    justify-content: space-between;
    margin-top: 20px
  }

  .logo-grid-item {
    display: flex;
    width: 100%; 
    flex-direction: column;
    align-items: center;
  }

  .logo-text {
    text-align: center;
    font-size: 16px;
    color: #262F69;
    margin-top: 16px;
    font-weight: 700;
  }

  .logo-grid-item img {
    width: auto;
    max-height: 60px;
  }
  
  .logo-text {
    
  }

  
  }

</style>


<script src="https://code.jquery.com/jquery-3.6.3.min.js"></script>
{{ 'bundle-page.css' | asset_url | stylesheet_tag }}
<div class="bundle_nd_sv imagetextwith" style="background:{{ section.settings.tp_bg_clr }};">
  <div class="bunld_cont">
    <div class="bundle-hero">
    <div class="bundle_text">
      <div class="hidden-background"></div>
      <span class="bundle-top-heading">{{ section.settings.top_heading }}</span>
      <h2 class="bundle-main-heading">{{ section.settings.title }}</h2>
      <span class="bundle-bottom-heading">{{ section.settings.bottom_heading }}</span>
      <span class="bundle-description">{{ section.settings.description }}</span>
      {% if section.settings.btn_text != blank %}
        <a class="secnd_btnshbnl" href="{{ section.settings.btn_url }}">{{ section.settings.btn_text }}</a>
        {% endif %}
        <div class="hero-logo-grid">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when "hero-logo-item" %}
                <div class="logo-grid-item">
                  <img src="{{ block.settings.logo_image | img_url: "master" }}" />
                  <span class="logo-text">{{ block.settings.text }}</span>
                </div>
              {% endcase %}
            {% endfor %}
        </div>
    </div>
      
        <img class="floating-hero-image" src="{{ section.settings.floating_img | img_url:'master' }}"> 
    <div class="bundle_img" style="width: 100%">
      <img class="desktop-hero-image" src="{{ section.settings.img | img_url:'master' }}">
      <img class="mobile-hero-image" src="{{ section.settings.mobile_img | img_url:'master' }}"> 
    </div>
    </div>
  </div>
</div>
<div>
  <div class="routine-grid-header">
    <h2 class="routine--main-text routine-header-text">{{ section.settings.routine_header }}</h2>
  </div>
  <div class="routine-grid">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'routine-card'%}
          <div class="routine-card">
            <img src="{{ block.settings.image | img_url: "master"}}" />
            <p class="routine--main-text">{{ block.settings.main_text }}</p>
            <span class="routine--body-text">{{ block.settings.body_text }}</span>
          </div>
      {% endcase %}
    {% endfor %}
  </div>
</div>
<div class="bundle_nd_sv fav_bndl">
  <h2>{{ section.settings.fav_heading }}</h2>
  <div class="collection_list">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'shopbundle' %}
          <div class="coll_grid">
            <h3>{{ block.settings.shop_ttl }}</h3>
            {% if block.settings.shop_img != blank %}<img {% if forloop.index == 3 %}style="aspect-ratio: 343/213; object-fit: contain;"{% endif %} src="{{ block.settings.shop_img | img_url:'master' }}">{% endif %}
            <div class="prod_btns">
              {% if block.settings.shop_btn_1 != blank %}
                <a href="{{ block.settings.shop_btnurl_1 }}" class="fsr_btnshbnl">
                  {{ block.settings.shop_btn_1 }}
                </a>
              {% endif %}
              {% if block.settings.shop_btn_2 != blank %}
                <a href="{{ block.settings.shop_btnurl_2 }}" class="secnd_btnshbnl">
                  {{ block.settings.shop_btn_2 }}
                </a>
              {% endif %}
            </div>
          </div>
      {% endcase %}
    {% endfor %}
  </div>
</div>

{% comment %}
<div class="product-grid">
  {% for product in section.settings.collection_test.products %}
    <div style="
      background: {{ product.metafields.custom.bundle_background_color }};
      color: {{ product.metafields.custom.bundle_secondary_text_color }};
      ">
      <h2>{{ product.title }}</h2>
      <div style="display: grid; grid-template-columns: 55% 45%;">
        <div>
          <img src="{{ product.featured_image | img_url: "master" }}" />
        </div>
        <div>
          <div style="
            background: {{ product.metafields.custom.bundle_page_dark_color }};
            color: {{ product.metafields.custom.bundle_secondary_text_color }};
            ">
          <input type="button" id="add-to-cart-button" value="ADD TO CART" onClick="addItemToCart({{ product.first_available_variant.id }}, 1, {{product.selling_plan_groups[0].selling_plans[0].id}})">       
          </div>
        </div>
      </div>
    </div>
  {% endfor %}
</div>
{% endcomment %}
<div class="image-with-text">

  <div class="iwt-image-container">
    <img class="iwt-image-main" src="{{ section.settings.iwt_image | img_url: "master"}}" />
  </div>
  <div class="iwt-text-section">
    <h2 class="iwt-header">{{ section.settings.iwt_header }}</h2>
    <h2 class="iwt-sub-header">{{ section.settings.iwt_sub_header }}</h2>
    <div class="iwt-bullet-section">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when "iwt-bullet" %}
          <div class="iwt-bullet">
            <img src="{{ "checkmark-cta.png" | asset_url }}" />
            <span>{{ block.settings.iwt_bullet_text }}</span>
          </div>
      {% endcase %}
    {% endfor %}
    </div>
    <a class="iwt-button-link" href="{{ section.settings.iwt_button_link }}">{{ section.settings.iwt_button_text }}</a>
  </div>
  
</div>

