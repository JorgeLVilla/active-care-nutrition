<script>
  $(document).on('click','.lft_que_ans h3',function(){
    if($(this).next('.conte_ans').css('display') == 'none'){
      $('.conte_ans').slideUp();
      $('.que_ans svg').css('transform','rotate(0deg)');
      $(this).next('.conte_ans').slideDown();
      $(this).children('svg').css('transform','rotate(180deg)');
    }
    else{
      $(this).children('svg').css('transform','rotate(0deg)');
      $(this).next('.conte_ans').slideUp();
    }
  });
</script>
